gcc -I src/ -I src/peripherals/encoder/ -I src/peripherals/movement/ -I src/peripherals/PWM/ -I src/peripherals/serial/ -I src/peripherals/ultrasonic/ -I src/ARCH/i386_OS/ -I /usr/local/include/jit/ src/*.c src/ARCH/i386_OS/*.c -std=c99 -DARCH=1  -DMEASURING=0 -DRUN_VM=1 -DCOUNTING=0 -DCOUNTING_STACK=0 -Os -L/usr/local/lib/ -ljit -Wl,-rpath,/usr/local/lib/  -o bin/microvm
